{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "arrowFunctions": true,
        "classes": true
    },
    "env": {
        "es6": true
    },
    "rules": {
        "camelcase": 1,
        "comma-dangle": 2,
        "quotes": [
            2,
            "single"
        ]
    }
}